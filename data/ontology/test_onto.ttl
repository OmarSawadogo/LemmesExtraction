#################################################################
#                    LINKS (Relations)
#################################################################

# ================== LINK: Plant-Disease ==================
:Link_Plant_Disease rdf:type owl:Class ;
    rdfs:label "Link Plant Disease"@en ;
    rdfs:label "Lien Plante-Maladie"@fr ;
    rdfs:comment "Relation entre une plante et une maladie diagnostiquée"@fr .

# --- Object Properties pour les Links ---
:affectsPlant rdf:type owl:ObjectProperty ;
    rdfs:label "affects plant"@en ;
    rdfs:label "affecte la plante"@fr ;
    rdfs:domain :Hub_Disease ;
    rdfs:range :Hub_Plant .

:hasDisease rdf:type owl:ObjectProperty ;
    rdfs:label "has disease"@en ;
    rdfs:label "a la maladie"@fr ;
    rdfs:domain :Hub_Plant ;
    rdfs:range :Hub_Disease ;
    owl:inverseOf :affectsPlant .

:hasHealthStatus rdf:type owl:ObjectProperty ;
    rdfs:label "has health status"@en ;
    rdfs:label "a l'état de santé"@fr ;
    rdfs:domain :Hub_Plant ;
    rdfs:range :Hub_HealthStatus .

# ================== LINK: Diagnosis ==================
:Link_Diagnosis rdf:type owl:Class ;
    rdfs:label "Link Diagnosis"@en ;
    rdfs:label "Lien Diagnostic"@fr ;
    rdfs:comment "Relation représentant un diagnostic effectué"@fr .

:diagnosedPlant rdf:type owl:ObjectProperty ;
    rdfs:label "diagnosed plant"@en ;
    rdfs:label "plante diagnostiquée"@fr ;
    rdfs:domain :Link_Diagnosis ;
    rdfs:range :Hub_Plant .

:diagnosedDisease rdf:type owl:ObjectProperty ;
    rdfs:label "diagnosed disease"@en ;
    rdfs:label "maladie diagnostiquée"@fr ;
    rdfs:domain :Link_Diagnosis ;
    rdfs:range :Hub_Disease .

:diagnosedStatus rdf:type owl:ObjectProperty ;
    rdfs:label "diagnosed status"@en ;
    rdfs:label "état diagnostiqué"@fr ;
    rdfs:domain :Link_Diagnosis ;
    rdfs:range :Hub_HealthStatus .

