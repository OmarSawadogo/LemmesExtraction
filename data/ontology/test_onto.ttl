<http://www.semanticweb.org/ontologies/2025/plantontology/> rdf:type owl:Ontology ;
    dc:title "Plant Disease Ontology - Burkina Faso"@en ;
    dc:title "Ontologie des Maladies des Plantes - Burkina Faso"@fr ;
    dc:description "Ontologie structurée selon le modèle Data Vault pour le diagnostic des maladies du maïs, de la tomate et de l'oignon au Burkina Faso"@fr ;
    dc:creator "SOS-LAMI" ;
    owl:versionInfo "2.0.0" .

#################################################################
#                         HUBS (Entités Métier)
#################################################################

# ================== HUB: Plant ==================
:Hub_Plant rdf:type owl:Class ;
    rdfs:label "Hub Plant"@en ;
    rdfs:label "Hub Plante"@fr ;
    rdfs:comment "Hub central représentant les types de plantes cultivées"@fr .

:Corn rdf:type owl:Class ;
    rdfs:subClassOf :Hub_Plant ;
    rdfs:label "Corn"@en ;
    rdfs:label "Maïs"@fr ;
    owl:disjointWith :Onion, :Tomato .

:Onion rdf:type owl:Class ;
    rdfs:subClassOf :Hub_Plant ;
    rdfs:label "Onion"@en ;
    rdfs:label "Oignon"@fr ;
    owl:disjointWith :Tomato .

:Tomato rdf:type owl:Class ;
    rdfs:subClassOf :Hub_Plant ;
    rdfs:label "Tomato"@en ;
    rdfs:label "Tomate"@fr .

# ================== HUB: Disease ==================
:Hub_Disease rdf:type owl:Class ;
    rdfs:label "Hub Disease"@en ;
    rdfs:label "Hub Maladie"@fr ;
    rdfs:comment "Hub central représentant les maladies des plantes"@fr .

# --- Catégories de maladies ---
:FungalDisease rdf:type owl:Class ;
    rdfs:subClassOf :Hub_Disease ;
    rdfs:label "Fungal Disease"@en ;
    rdfs:label "Maladie Fongique"@fr .

:BacterialDisease rdf:type owl:Class ;
    rdfs:subClassOf :Hub_Disease ;
    rdfs:label "Bacterial Disease"@en ;
    rdfs:label "Maladie Bactérienne"@fr .

:ViralDisease rdf:type owl:Class ;
    rdfs:subClassOf :Hub_Disease ;
    rdfs:label "Viral Disease"@en ;
    rdfs:label "Maladie Virale"@fr .

:AbioticStress rdf:type owl:Class ;
    rdfs:subClassOf :Hub_Disease ;
    rdfs:label "Abiotic Stress"@en ;
    rdfs:label "Stress Abiotique"@fr .

:Pest rdf:type owl:Class ;
    rdfs:subClassOf :Hub_Disease ;
    rdfs:label "Pest"@en ;
    rdfs:label "Ravageur"@fr .

# ================== HUB: HealthStatus ==================
:Hub_HealthStatus rdf:type owl:Class ;
    rdfs:label "Hub Health Status"@en ;
    rdfs:label "Hub État de Santé"@fr ;
    rdfs:comment "État de santé de la plante"@fr .

:Healthy rdf:type owl:Class ;
    rdfs:subClassOf :Hub_HealthStatus ;
    rdfs:label "Healthy"@en ;
    rdfs:label "Sain"@fr .

:Diseased rdf:type owl:Class ;
    rdfs:subClassOf :Hub_HealthStatus ;
    rdfs:label "Diseased"@en ;
    rdfs:label "Malade"@fr .

:Stressed rdf:type owl:Class ;
    rdfs:subClassOf :Hub_HealthStatus ;
    rdfs:label "Stressed"@en ;
    rdfs:label "Stressé"@fr .

