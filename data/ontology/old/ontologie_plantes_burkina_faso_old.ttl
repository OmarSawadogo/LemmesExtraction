@base <http://www.semanticweb.org/hp/ontologies/2025/10/planteontolgie/> . 

<http://www.semanticweb.org/hp/ontologies/2025/10/planteontolgie> rdf:type owl:Ontology ;
    rdfs:label "Ontologie des Plantes Agricoles du Burkina Faso - Version Data Vault"@fr ;
    rdfs:comment "Ontologie enrichie pour extraction Data Vault (Hubs, Links, Satellites) - Systeme d'aide au diagnostic agricole"@fr ;
    dct:creator "SOS - LAMI, Universite Joseph KI-ZERBO" ;
    dct:created "2025"^^xsd:gYear ;
    owl:versionInfo "3.0 - Data Vault Ready" .

#################################################################
#    NAMESPACE DATA VAULT - ANNOTATIONS POUR EXTRACTION
#################################################################

dv:Hub rdf:type owl:AnnotationProperty ;
    rdfs:label "Hub Data Vault"@fr ;
    rdfs:comment "Indique une entite metier principale avec cle naturelle"@fr .

dv:Link rdf:type owl:AnnotationProperty ;
    rdfs:label "Link Data Vault"@fr ;
    rdfs:comment "Indique une relation entre plusieurs Hubs"@fr .

dv:Satellite rdf:type owl:AnnotationProperty ;
    rdfs:label "Satellite Data Vault"@fr ;
    rdfs:comment "Indique des attributs descriptifs rattaches a un Hub ou Link"@fr .

dv:businessKey rdf:type owl:AnnotationProperty ;
    rdfs:label "Cle metier"@fr ;
    rdfs:comment "Propriete servant de cle metier naturelle pour le Hub"@fr .

dv:parentHub rdf:type owl:AnnotationProperty ;
    rdfs:label "Hub parent"@fr ;
    rdfs:comment "Reference au Hub auquel ce Satellite est rattache"@fr .

dv:linkedHubs rdf:type owl:AnnotationProperty ;
    rdfs:label "Hubs lies"@fr ;
    rdfs:comment "Liste des Hubs connectes par ce Link"@fr .

dv:loadDate rdf:type owl:AnnotationProperty ;
    rdfs:label "Date de chargement"@fr ;
    rdfs:comment "Timestamp du chargement dans le Data Vault"@fr .

dv:recordSource rdf:type owl:AnnotationProperty ;
    rdfs:label "Source d'enregistrement"@fr ;
    rdfs:comment "Origine des donnees"@fr .

dv:hashDiff rdf:type owl:AnnotationProperty ;
    rdfs:label "Hash de difference"@fr ;
    rdfs:comment "Hash pour detecter les changements dans les Satellites"@fr .

#################################################################
#    CLASSES HUBS - ENTITES METIER PRINCIPALES
#################################################################

# ================= HUB: PLANTE =================
:plante rdf:type owl:Class ;
    rdfs:label "Plante"@fr ;
    rdfs:comment "Hub principal - Culture agricole avec cle metier nom_scientifique"@fr ;
    dv:Hub "true"^^xsd:boolean ;
    dv:businessKey ":nom_scientifique" ;
    skos:definition "Entite centrale representant une espece vegetale cultivee"@fr .

:mais rdf:type owl:Class ;
    rdfs:subClassOf :plante ;
    rdfs:label "Mais"@fr ;
    rdfs:comment "Zea mays - Culture cerealiere majeure au Burkina Faso"@fr ;
    skos:altLabel "Kamaana"@moore , "Corn"@en ;
    owl:disjointWith :oignon , :tomate .

:oignon rdf:type owl:Class ;
    rdfs:subClassOf :plante ;
    rdfs:label "Oignon"@fr ;
    rdfs:comment "Allium cepa - Culture maraichere"@fr ;
    skos:altLabel "Gyabga"@moore , "Onion"@en ;
    owl:disjointWith :tomate . 

:tomate rdf:type owl:Class ;
    rdfs:subClassOf :plante ;
    rdfs:label "Tomate"@fr ;
    rdfs:comment "Solanum lycopersicum - Culture maraichere"@fr ;
    skos:altLabel "Tomaate"@moore , "Tomato"@en .

:mil rdf:type owl:Class ;
    rdfs:subClassOf :plante ;
    rdfs:label "Mil"@fr ;
    rdfs:comment "Pennisetum glaucum - Cereale traditionnelle resistante a la secheresse"@fr ;
    skos:altLabel "Kazanga"@moore , "Pearl millet"@en .

:sorgho rdf:type owl:Class ;
    rdfs:subClassOf :plante ;
    rdfs:label "Sorgho"@fr ;
    rdfs:comment "Sorghum bicolor - Cereale adaptee aux zones semi-arides"@fr ;
    skos:altLabel "Ki"@moore , "Sorghum"@en .

:niebe rdf:type owl:Class ;
    rdfs:subClassOf :plante ;
    rdfs:label "Niebe"@fr ;
    rdfs:comment "Vigna unguiculata - Legumineuse riche en proteines"@fr ;
    skos:altLabel "Benga"@moore , "Cowpea"@en .

:arachide rdf:type owl:Class ;
    rdfs:subClassOf :plante ;
    rdfs:label "Arachide"@fr ;
    rdfs:comment "Arachis hypogaea - Oleagineuse importante"@fr ;
    skos:altLabel "Groundnut"@en .

:coton rdf:type owl:Class ;
    rdfs:subClassOf :plante ;
    rdfs:label "Coton"@fr ;
    rdfs:comment "Gossypium hirsutum - Culture de rente majeure"@fr ;
    skos:altLabel "Cotton"@en .

# ================= HUB: MALADIE =================
:maladie rdf:type owl:Class ;
    rdfs:label "Maladie"@fr ;
    rdfs:comment "Hub - Probleme phytosanitaire avec cle metier code_maladie"@fr ;
    dv:Hub "true"^^xsd:boolean ;
    dv:businessKey ":code_maladie" ;
    skos:definition "Affection ou stress affectant la sante des plantes"@fr .

:maladie_fongique rdf:type owl:Class ;
    rdfs:subClassOf :maladie ;
    rdfs:label "Maladie fongique"@fr ;
    rdfs:comment "Maladie causee par des champignons pathogenes"@fr .

:maladie_bacterienne rdf:type owl:Class ;
    rdfs:subClassOf :maladie ;
    rdfs:label "Maladie bacterienne"@fr ;
    rdfs:comment "Maladie causee par des bacteries phytopathogenes"@fr . 

:maladie_virale rdf:type owl:Class ;
    rdfs:subClassOf :maladie ;
    rdfs:label "Maladie virale"@fr ;
    rdfs:comment "Maladie causee par des virus vegetaux"@fr .

:ravageur rdf:type owl:Class ;
    rdfs:subClassOf :maladie ;
    rdfs:label "Ravageur"@fr ;
    rdfs:comment "Insectes, acariens et autres organismes nuisibles aux cultures"@fr .

:stress_abiotique rdf:type owl:Class ;
    rdfs:subClassOf :maladie ;
    rdfs:label "Stress abiotique"@fr ;
    rdfs:comment "Stress cause par des facteurs non-vivants"@fr .

:carence_nutritionnelle rdf:type owl:Class ;
    rdfs:subClassOf :stress_abiotique ;
    rdfs:label "Carence nutritionnelle"@fr ;
    rdfs:comment "Deficience en elements nutritifs essentiels"@fr .

:stress_hydrique rdf:type owl:Class ;
    rdfs:subClassOf :stress_abiotique ;
    rdfs:label "Stress hydrique"@fr ;
    rdfs:comment "Manque ou exces d'eau affectant la plante"@fr .

:stress_thermique rdf:type owl:Class ;
    rdfs:subClassOf :stress_abiotique ;
    rdfs:label "Stress thermique"@fr ;
    rdfs:comment "Temperature excessive ou insuffisante"@fr .

# ================= HUB: REGION =================
:region rdf:type owl:Class ;
