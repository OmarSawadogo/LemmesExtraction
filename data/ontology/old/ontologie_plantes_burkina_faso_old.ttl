#################################################################
#    RELATIONS SYMPTOMES - COULEURS
#################################################################

:necrose :modifie_couleur :beige_brun ;
    :affecte_organe :feuille , :tige .

:chlorose :modifie_couleur :jaunatre , :vert_clair ;
    :affecte_organe :feuille .

:fletrissement :affecte_organe :feuille , :tige .

:taches_foliaires :modifie_couleur :taches_brunes , :couleur_rouille ;
    :affecte_organe :feuille .

:perforation :affecte_organe :feuille , :tige , :epi .

:striures :modifie_couleur :jaunatre ;
    :affecte_organe :feuille .

:pourriture :affecte_organe :racine_organe , :bulbe_organe , :fruit , :collet .

:enroulement :affecte_organe :feuille .

:galle :affecte_organe :tige , :epi , :racine_organe .

:mosaique :modifie_couleur :jaunatre , :vert_clair ;
    :affecte_organe :feuille .

