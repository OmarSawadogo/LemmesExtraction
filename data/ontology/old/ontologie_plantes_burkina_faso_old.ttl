#################################################################
#    CLASSES LINKS - RELATIONS ENTRE HUBS
#################################################################

# ================= LINK: DIAGNOSTIC =================
:link_diagnostic rdf:type owl:Class ;
    rdfs:label "Link Diagnostic"@fr ;
