    dv:Link "true"^^xsd:boolean ;
    dv:linkedHubs ":image, :plante, :maladie, :symptome" ;
    skos:definition "Association entre une observation et un diagnostic"@fr .

# ================= LINK: OBSERVATION =================
:link_observation rdf:type owl:Class ;
