@prefix :  <http://www.semanticweb.org/hp/ontologies/2025/10/planteontolgie/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/hp/ontologies/2025/10/planteontolgie/> . 

<http://www.semanticweb.org/hp/ontologies/2025/10/planteontolgie> rdf:type owl:Ontology ;
    rdfs:label "Ontologie des Plantes Agricoles du Burkina Faso"@fr ;
    rdfs:comment "Ontologie pour les donnees d images de plantes de mais, oignon et tomate au Burkina Faso"@fr . 

#################################################################
#    CLASSES PRINCIPALES
#################################################################

:plante rdf:type owl:Class ;
    rdfs:label "Plante"@fr ;
    rdfs:comment "Classe parente de toutes les cultures agricoles"@fr .

:mais rdf:type owl:Class ;
    rdfs:subClassOf :plante ;
    rdfs:label "Mais"@fr ;
    rdfs:comment "Zea mays - Culture cerealiere majeure au Burkina Faso"@fr ;
    owl:disjointWith :oignon , :tomate .

:oignon rdf:type owl:Class ;
    rdfs:subClassOf :plante ;
    rdfs:label "Oignon"@fr ;
    rdfs:comment "Allium cepa - Culture maraichere"@fr ;
    owl:disjointWith :tomate . 

:tomate rdf:type owl:Class ;
    rdfs:subClassOf :plante ;
    rdfs:label "Tomate"@fr ;
    rdfs:comment "Solanum lycopersicum - Culture maraichere"@fr . 

:maladie rdf:type owl:Class ;
    rdfs:label "Maladie"@fr ;
    rdfs:comment "Classe representant les maladies et problemes phytosanitaires des plantes"@fr .

:maladie_fongique rdf:type owl:Class ;
    rdfs:subClassOf :maladie ;
    rdfs:label "Maladie fongique"@fr ;
    rdfs:comment "Maladie causee par des champignons"@fr .

:maladie_bacterienne rdf:type owl:Class ;
    rdfs:subClassOf :maladie ;
    rdfs:label "Maladie bacterienne"@fr ;
    rdfs:comment "Maladie causee par des bacteries"@fr . 

:maladie_virale rdf:type owl:Class ;
    rdfs:subClassOf :maladie ;
    rdfs:label "Maladie virale"@fr ;
    rdfs:comment "Maladie causee par des virus"@fr .

:ravageur rdf:type owl:Class ;
    rdfs:subClassOf :maladie ;
    rdfs:label "Ravageur"@fr ;
    rdfs:comment "Insectes, acariens et autres organismes nuisibles aux cultures"@fr .

:stress_abiotique rdf:type owl:Class ;
    rdfs:subClassOf :maladie ;
    rdfs:label "Stress abiotique"@fr ;
    rdfs:comment "Stress cause par des facteurs non-vivants eau nutriments temperature sol"@fr .

#################################################################
#    CLASSES MORPHOLOGIQUES
#################################################################

:type_plante rdf:type owl:Class ;
    rdfs:label "Type de plante"@fr ;
    rdfs:comment "Classification botanique de la plante"@fr .

:monocotyledone rdf:type owl:Class ;
    rdfs:subClassOf :type_plante ;
    rdfs:label "Monocotyledone"@fr ;
    rdfs:comment "Plante a une seule feuille embryonnaire cotyledon"@fr .

:dicotyledone rdf:type owl:Class ;
    rdfs:subClassOf :type_plante ;
    rdfs:label "Dicotyledone"@fr ;
    rdfs:comment "Plante a deux feuilles embryonnaires cotyledons"@fr .

:famille_botanique rdf:type owl:Class ;
    rdfs:label "Famille botanique"@fr ;
    rdfs:comment "Classification taxonomique de la famille de la plante"@fr . 

:poaceae rdf:type owl:Class ;
    rdfs:subClassOf :famille_botanique ;
    rdfs:label "Poacees Graminees"@fr ;
    rdfs:comment "Famille des graminees incluant le mais le mil le sorgho"@fr .

:solanaceae rdf:type owl:Class ;
    rdfs:subClassOf :famille_botanique ;
    rdfs:label "Solanacees"@fr ;
    rdfs:comment "Famille incluant la tomate le piment l aubergine"@fr .

:amaryllidaceae rdf:type owl:Class ;
    rdfs:subClassOf :famille_botanique ;
    rdfs:label "Amaryllidacees"@fr ;
    rdfs:comment "Famille incluant l oignon l ail le poireau"@fr .

:forme_feuille rdf:type owl:Class ;
    rdfs:label "Forme de feuille"@fr ;
    rdfs:comment "Morphologie et forme generale de la feuille"@fr .

:lineaire_lanceolee rdf:type owl:Class ;
    rdfs:subClassOf :forme_feuille ;
    rdfs:label "Lineaire-lanceolee"@fr ;
    rdfs:comment "Feuille longue etroite et pointue typique du mais"@fr .

:composee_pennee rdf:type owl:Class ;
    rdfs:subClassOf :forme_feuille ;
    rdfs:label "Composee-pennee"@fr ;
    rdfs:comment "Feuille divisee en folioles disposees comme une plume typique de la tomate"@fr . 

:tubulaire rdf:type owl:Class ;
    rdfs:subClassOf :forme_feuille ;
    rdfs:label "Tubulaire"@fr ;
    rdfs:comment "Feuille creuse et cylindrique typique de l oignon"@fr .

:simple rdf:type owl:Class ;
    rdfs:subClassOf :forme_feuille ;
    rdfs:label "Simple"@fr ;
    rdfs:comment "Feuille non divisee"@fr .

:lobee rdf:type owl:Class ;
    rdfs:subClassOf :forme_feuille ;
    rdfs:label "Lobee"@fr ;
    rdfs:comment "Feuille avec des lobes"@fr .

:dentee rdf:type owl:Class ;
    rdfs:subClassOf :forme_feuille ;
    rdfs:label "Dentee"@fr ;
    rdfs:comment "Feuille avec des bords en forme de dents"@fr .

:couleur_feuille rdf:type owl:Class ;
    rdfs:label "Couleur de feuille"@fr ;
    rdfs:comment "Couleur observee sur les feuilles de la plante"@fr . 

:vert_fonce rdf:type owl:Class ;
    rdfs:subClassOf :couleur_feuille ;
    rdfs:label "Vert fonce"@fr ;
    rdfs:comment "Couleur normale d une plante saine et bien nourrie"@fr .

:vert_clair rdf:type owl:Class ;
    rdfs:subClassOf :couleur_feuille ;
    rdfs:label "Vert clair"@fr ;
    rdfs:comment "Couleur plus claire peut indiquer un debut de stress"@fr .

:vert_moyen rdf:type owl:Class ;
    rdfs:subClassOf :couleur_feuille ;
    rdfs:label "Vert moyen"@fr ;
    rdfs:comment "Couleur verte intermediaire"@fr .

:jaunatre rdf:type owl:Class ;
    rdfs:subClassOf :couleur_feuille ;
    rdfs:label "Jaunatre"@fr ;
    rdfs:comment "Couleur jaune indiquant une chlorose ou un stress nutritionnel"@fr .

:beige_brun rdf:type owl:Class ;
    rdfs:subClassOf :couleur_feuille ;
    rdfs:label "Beige Brun"@fr ;
    rdfs:comment "Couleur indiquant une necrose ou mort des tissus"@fr .

:taches_brunes rdf:type owl:Class ;
    rdfs:subClassOf :couleur_feuille ;
    rdfs:label "Taches brunes"@fr ;
    rdfs:comment "Presence de taches brunes sur fond vert"@fr .

:couleur_rouille rdf:type owl:Class ;
    rdfs:subClassOf :couleur_feuille ;
    rdfs:label "Couleur rouille"@fr ;
    rdfs:comment "Couleur orange-brun caracteristique de la rouille"@fr .

:texture_feuille rdf:type owl:Class ;
    rdfs:label "Texture de feuille"@fr ;
    rdfs:comment "Caracteristiques tactiles et visuelles de la surface foliaire"@fr .

:lisse rdf:type owl:Class ;
    rdfs:subClassOf :texture_feuille ;
    rdfs:label "Lisse"@fr ;
    rdfs:comment "Surface foliaire sans asperites"@fr .

:pubescente rdf:type owl:Class ;
    rdfs:subClassOf :texture_feuille ;
    rdfs:label "Pubescente"@fr ;
    rdfs:comment "Surface foliaire couverte de petits poils"@fr .

:cireuse rdf:type owl:Class ;
    rdfs:subClassOf :texture_feuille ;
    rdfs:label "Cireuse"@fr ;
    rdfs:comment "Surface foliaire recouverte d une couche cireuse protectrice"@fr .

:rugueuse rdf:type owl:Class ;
    rdfs:subClassOf :texture_feuille ;
    rdfs:label "Rugueuse"@fr ;
    rdfs:comment "Surface foliaire irreguliere au toucher"@fr .

:nervation rdf:type owl:Class ;
    rdfs:label "Nervation"@fr ;
    rdfs:comment "Disposition des nervures sur la feuille"@fr .

:nervation_parallele rdf:type owl:Class ;
    rdfs:subClassOf :nervation ;
    rdfs:label "Nervation parallele"@fr ;
    rdfs:comment "Nervures disposees parallelement caracteristique des monocotyledones"@fr . 

:nervation_reticulee rdf:type owl:Class ;
    rdfs:subClassOf :nervation ;
    rdfs:label "Nervation reticulee"@fr ;
    rdfs:comment "Nervures formant un reseau caracteristique des dicotyledones"@fr .

:dimension_feuille rdf:type owl:Class ;
    rdfs:label "Dimension de feuille"@fr ;
    rdfs:comment "Taille generale de la feuille"@fr .

:feuille_grande rdf:type owl:Class ;
    rdfs:subClassOf :dimension_feuille ;
    rdfs:label "Grande feuille 50-100 cm"@fr ;
    rdfs:comment "Feuille de grande taille typique du mais"@fr . 

:feuille_moyenne rdf:type owl:Class ;
    rdfs:subClassOf :dimension_feuille ;
    rdfs:label "Feuille moyenne 20-50 cm"@fr ;
    rdfs:comment "Feuille de taille moyenne typique de la tomate"@fr .

:feuille_petite rdf:type owl:Class ;
    rdfs:subClassOf :dimension_feuille ;
    rdfs:label "Petite feuille moins de 20 cm"@fr ;
    rdfs:comment "Feuille de petite taille"@fr .

:type_tige rdf:type owl:Class ;
    rdfs:label "Type de tige"@fr ;
    rdfs:comment "Caracteristiques morphologiques de la tige"@fr .

:chaume rdf:type owl:Class ;
    rdfs:subClassOf :type_tige ;
    rdfs:label "Chaume"@fr ;
    rdfs:comment "Tige cylindrique creuse des graminees comme le mais"@fr .

:tige_herbacee rdf:type owl:Class ;
    rdfs:subClassOf :type_tige ;
    rdfs:label "Tige herbacee"@fr ;
    rdfs:comment "Tige souple et non ligneuse"@fr .

:tige_bulbeuse rdf:type owl:Class ;
    rdfs:subClassOf :type_tige ;
    rdfs:label "Tige bulbeuse"@fr ;
    rdfs:comment "Tige modifiee en bulbe comme chez l oignon"@fr .

:type_racine rdf:type owl:Class ;
    rdfs:label "Type de racine"@fr ;
    rdfs:comment "Systeme racinaire de la plante"@fr . 

:racine_fasciculee rdf:type owl:Class ;
    rdfs:subClassOf :type_racine ;
    rdfs:label "Racine fasciculee"@fr ;
    rdfs:comment "Racines en touffe sans racine principale typique des monocotyledones"@fr .

:racine_pivotante rdf:type owl:Class ;
    rdfs:subClassOf :type_racine ;
    rdfs:label "Racine pivotante"@fr ;
    rdfs:comment "Racine principale dominante avec ramifications typique des dicotyledones"@fr .

:bulbe rdf:type owl:Class ;
    rdfs:subClassOf :type_racine ;
    rdfs:label "Bulbe"@fr ;
    rdfs:comment "Organe de reserve souterrain comme chez l oignon"@fr . 

:symptome rdf:type owl:Class ;
    rdfs:label "Symptome"@fr ;
    rdfs:comment "Signe visible d une maladie ou d un stress sur la plante"@fr . 

:necrose rdf:type owl:Class ;
    rdfs:subClassOf :symptome ;
    rdfs:label "Necrose"@fr ;
    rdfs:comment "Mort des tissus vegetaux zones brunes ou beiges"@fr .

:chlorose rdf:type owl:Class ;
    rdfs:subClassOf :symptome ;
    rdfs:label "Chlorose"@fr ;
    rdfs:comment "Jaunissement des feuilles du a un manque de chlorophylle"@fr . 

:fletrissement rdf:type owl:Class ;
    rdfs:subClassOf :symptome ;
    rdfs:label "Fletrissement"@fr ;
    rdfs:comment "Perte de turgescence des tissus vegetaux"@fr . 

:taches_foliaires rdf:type owl:Class ;
    rdfs:subClassOf :symptome ;
    rdfs:label "Taches foliaires"@fr ;
    rdfs:comment "Presence de taches de differentes couleurs sur les feuilles"@fr . 

:perforation rdf:type owl:Class ;
    rdfs:subClassOf :symptome ;
    rdfs:label "Perforation"@fr ;
    rdfs:comment "Trous dans les feuilles causes par des ravageurs"@fr .

:deformation rdf:type owl:Class ;
    rdfs:subClassOf :symptome ;
    rdfs:label "Deformation"@fr ;
    rdfs:comment "Alteration de la forme normale des organes de la plante"@fr . 

:pourriture rdf:type owl:Class ;
    rdfs:subClassOf :symptome ;
    rdfs:label "Pourriture"@fr ;
    rdfs:comment "Decomposition des tissus vegetaux"@fr .

:nanisme rdf:type owl:Class ;
    rdfs:subClassOf :symptome ;
    rdfs:label "Nanisme"@fr ;
    rdfs:comment "Reduction anormale de la taille de la plante"@fr . 

:striures rdf:type owl:Class ;
    rdfs:subClassOf :symptome ;
    rdfs:label "Striures"@fr ;
    rdfs:comment "Lignes ou bandes de couleur differente sur les feuilles"@fr . 

:etat_sanitaire rdf:type owl:Class ;
    rdfs:label "Etat sanitaire"@fr ;
    rdfs:comment "Etat de sante general de la plante"@fr . 

:sain rdf:type owl:Class ;
    rdfs:subClassOf :etat_sanitaire ;
    rdfs:label "Sain"@fr ;
    rdfs:comment "Plante en bonne sante sans symptomes de maladie"@fr . 

:malade rdf:type owl:Class ;
    rdfs:subClassOf :etat_sanitaire ;
    rdfs:label "Malade"@fr ;
    rdfs:comment "Plante presentant des symptomes de maladie"@fr .

:stresse rdf:type owl:Class ;
    rdfs:subClassOf :etat_sanitaire ;
    rdfs:label "Stresse"@fr ;
    rdfs:comment "Plante soumise a un stress environnemental"@fr .

:cycle_croissance rdf:type owl:Class ;
    rdfs:label "Cycle de croissance"@fr ;
    rdfs:comment "Duree de vie de la plante"@fr . 

:annuel rdf:type owl:Class ;
    rdfs:subClassOf :cycle_croissance ;
    rdfs:label "Annuel"@fr ;
    rdfs:comment "Plante completant son cycle en une annee ou moins"@fr .

:bisannuel rdf:type owl:Class ;
    rdfs:subClassOf :cycle_croissance ;
    rdfs:label "Bisannuel"@fr ;
    rdfs:comment "Plante completant son cycle en deux annees"@fr .

:vivace rdf:type owl:Class ;
    rdfs:subClassOf :cycle_croissance ;
    rdfs:label "Vivace"@fr ;
    rdfs:comment "Plante vivant plusieurs annees"@fr .

:port_plante rdf:type owl:Class ;
    rdfs:label "Port de la plante"@fr ;
    rdfs:comment "Allure generale et mode de croissance de la plante"@fr .

:dresse rdf:type owl:Class ;
    rdfs:subClassOf :port_plante ;
    rdfs:label "Dresse"@fr ;
    rdfs:comment "Plante a croissance verticale"@fr .

:rampant rdf:type owl:Class ;
    rdfs:subClassOf :port_plante ;
    rdfs:label "Rampant"@fr ;
    rdfs:comment "Plante a croissance horizontale au sol"@fr .

:grimpant rdf:type owl:Class ;
    rdfs:subClassOf :port_plante ;
    rdfs:label "Grimpant"@fr ;
    rdfs:comment "Plante necessitant un support pour grimper"@fr .

