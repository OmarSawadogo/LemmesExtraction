#################################################################
#    INSTANCES DE MALADIES
#################################################################

:helminthosporiose_instance rdf:type owl:NamedIndividual , :maladie_fongique ;
    rdfs:label "Helminthosporiose Brulure foliaire du Nord"@fr ;
    rdfs:comment "Causee par le champignon Exserohilum turcicum provoque des lesions allongees beiges brunes sur les feuilles"@fr ;
    :cause_symptome :necrose ;
    :affecte_culture :mais . 

:chenille_legionnaire_instance rdf:type owl:NamedIndividual , :ravageur ;
    rdfs:label "Chenille legionnaire d automne"@fr ;
    rdfs:comment "Spodoptera frugiperda Ravageur majeur des cultures cerealieres au Burkina Faso"@fr ;
    :cause_symptome :perforation ;
