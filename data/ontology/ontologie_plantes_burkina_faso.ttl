    rdfs:label "Image"@fr .

:Observation rdf:type owl:Class ;
    rdfs:label "Observation"@fr ;
    rdfs:comment "Classe de liaison pour une détection/identification" .


#################################################################
#    OBJECT PROPERTIES (LINKS)
#################################################################

### ========================================================
### Relations Plante <-> Parties
### ========================================================

:has_part rdf:type owl:ObjectProperty ;
    rdfs:domain :Plant ;
    rdfs:range :PlantPart ;
    rdfs:label "a pour partie"@fr ;
    :lemma "has_part" .

:is_part_of rdf:type owl:ObjectProperty ;
    owl:inverseOf :has_part ;
    rdfs:domain :PlantPart ;
    rdfs:range :Plant ;
    rdfs:label "est partie de"@fr .

### ========================================================
### Relations Plante <-> Maladies (LINK PRINCIPAL)
### ========================================================

:has_disease rdf:type owl:ObjectProperty ;
    rdfs:domain :Plant ;
    rdfs:range :Disease ;
    rdfs:label "a pour maladie"@fr ;
    :isLink "true"^^xsd:boolean ;
    :lemma "has_disease" .

:affects_plant rdf:type owl:ObjectProperty ;
    owl:inverseOf :has_disease ;
    rdfs:domain :Disease ;
    rdfs:range :Plant ;
    rdfs:label "affecte la plante"@fr .

:affects_plant_part rdf:type owl:ObjectProperty ;
    rdfs:domain :Disease ;
    rdfs:range :PlantPart ;
    rdfs:label "affecte la partie"@fr .

### ========================================================
### Relations Plante <-> Ravageurs (LINK PRINCIPAL)
### ========================================================

:has_infestation rdf:type owl:ObjectProperty ;
    rdfs:domain :Plant ;
    rdfs:range :Pest ;
    rdfs:label "est infesté par"@fr ;
    :isLink "true"^^xsd:boolean ;
    :lemma "has_infestation" .

:attacks_plant rdf:type owl:ObjectProperty ;
    owl:inverseOf :has_infestation ;
    rdfs:domain :Pest ;
    rdfs:range :Plant ;
    rdfs:label "attaque la plante"@fr .

:attacks_plant_part rdf:type owl:ObjectProperty ;
    rdfs:domain :Pest ;
    rdfs:range :PlantPart ;
    rdfs:label "attaque la partie"@fr .

### ========================================================
### Relations État de santé (LINK PRINCIPAL)
### ========================================================

:has_health_status rdf:type owl:ObjectProperty ;
    rdfs:domain :Plant ;
    rdfs:range :HealthStatus ;
    rdfs:label "a pour état de santé"@fr ;
    :isLink "true"^^xsd:boolean ;
    :lemma "has_health_status" .

### ========================================================
### Relations Ravageurs <-> Maladies
### ========================================================

:transmits_disease rdf:type owl:ObjectProperty ;
    rdfs:domain :Pest ;
    rdfs:range :Disease ;
    rdfs:label "transmet la maladie"@fr .

:is_transmitted_by rdf:type owl:ObjectProperty ;
    owl:inverseOf :transmits_disease ;
    rdfs:domain :Disease ;
    rdfs:range :Pest ;
    rdfs:label "est transmise par"@fr .

:causes_disease rdf:type owl:ObjectProperty ;
    rdfs:domain :Pest ;
    rdfs:range :Disease ;
    rdfs:label "cause la maladie"@fr .

:favors_infestation rdf:type owl:ObjectProperty ;
    rdfs:domain :Disease ;
    rdfs:range :Pest ;
    rdfs:label "favorise l'infestation"@fr .

### ========================================================
### Relations Maladies/Ravageurs <-> Symptômes
### ========================================================

:has_symptom rdf:type owl:ObjectProperty ;
    rdfs:domain :Disease ;
    rdfs:range :Symptom ;
    rdfs:label "a pour symptôme"@fr .

:is_symptom_of rdf:type owl:ObjectProperty ;
    owl:inverseOf :has_symptom ;
    rdfs:domain :Symptom ;
    rdfs:range :Disease ;
    rdfs:label "est symptôme de"@fr .

:causes_damage rdf:type owl:ObjectProperty ;
    rdfs:domain :Pest ;
    rdfs:range :Symptom ;
    rdfs:label "cause le dégât"@fr .

:is_damage_caused_by rdf:type owl:ObjectProperty ;
