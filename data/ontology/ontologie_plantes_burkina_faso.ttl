<http://www.semanticweb.org/hp/ontologies/2025/10/planteontolgie> rdf:type owl:Ontology ;
    rdfs:comment "Ontologie Data Vault pour l'identification des plantes et diagnostic phytosanitaire au Burkina Faso"@fr ;
    rdfs:label "PlantOntoBF"@fr ;
    owl:versionInfo "3.0" .


#################################################################
#    ANNOTATION PROPERTIES (Métadonnées Data Vault)
#################################################################

:isHub rdf:type owl:AnnotationProperty ;
    rdfs:comment "Marque une classe comme HUB dans le modèle Data Vault" .

:isLink rdf:type owl:AnnotationProperty ;
    rdfs:comment "Marque une propriété comme LINK dans le modèle Data Vault" .

:isSatellite rdf:type owl:AnnotationProperty ;
    rdfs:comment "Marque une propriété ou classe comme SATELLITE dans le modèle Data Vault" .

:lemma rdf:type owl:AnnotationProperty ;
    rdfs:comment "Lemme standardisé pour l'extraction (sans accents, underscores)" .


#################################################################
#    CLASSES PRINCIPALES (HUBS)
#################################################################

### ========================================================
### PLANTES (HUBS PRINCIPAUX)
### ========================================================

:Plant rdf:type owl:Class ;
    rdfs:label "Plante"@fr, "Plant"@en ;
    rdfs:comment "Classe parente pour toutes les plantes cultivées" ;
    :isHub "true"^^xsd:boolean .

:Corn rdf:type owl:Class ;
    rdfs:subClassOf :Plant ;
    rdfs:label "Maïs"@fr, "Corn"@en ;
    rdfs:comment "Zea mays - Céréale de la famille des Poacées"@fr ;
    :lemma "corn" ;
    :isHub "true"^^xsd:boolean ;
    owl:disjointWith :Onion, :Tomato .

:Onion rdf:type owl:Class ;
    rdfs:subClassOf :Plant ;
    rdfs:label "Oignon"@fr, "Onion"@en ;
    rdfs:comment "Allium cepa - Légume bulbe de la famille des Amaryllidacées"@fr ;
    :lemma "onion" ;
    :isHub "true"^^xsd:boolean ;
    owl:disjointWith :Tomato .

:Tomato rdf:type owl:Class ;
    rdfs:subClassOf :Plant ;
    rdfs:label "Tomate"@fr, "Tomato"@en ;
    rdfs:comment "Solanum lycopersicum - Légume fruit de la famille des Solanacées"@fr ;
    :lemma "tomato" ;
    :isHub "true"^^xsd:boolean .

